        "# 품사 태깅\n",
        "import nltk\n",
        "from nltk import pos_tag\n",
        "from nltk import word_tokenize\n",
        "\n",
        "nltk.download('punkt')\n",
        "nltk.download('averaged_perceptron_tagger') # 태거 다운로드\n",
        "\n",
        "text = \"Rachel loves drinking coffee in the morning\""
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "tagged_text = pos_tag(word_tokenize(text))\n",
        "print(tagged_text)"
